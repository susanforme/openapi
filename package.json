{
  "name": "openapi",
  "version": "1.0.1",
  "description": "",
  "type": "module",
  "types": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "esno ./src/index.ts",
    "build": "rimraf dist && run-s build-bundle",
    "build-bundle": "rollup --config rollup.config.ts --configPlugin typescript &&node scripts/mv",
    "typecheck": "tsc --noEmit",
    "type": " tsc -p ./tsconfig.json --emitDeclarationOnly",
    "tsc": "tsc -p ./tsconfig.json",
    "lint": "eslint --ext .ts src/**",
    "format": "prettier --write src/**",
    "prepublish": "run-s build typecheck lint format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/babel__generator": "^7.6.8",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "esno": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^3.17.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.1.0",
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "node-fetch": "^3.3.2",
    "ts-factory-code-generator-generator": "^0.7.0"
  }
}